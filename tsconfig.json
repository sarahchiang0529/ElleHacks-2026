{
    "compilerOptions": {
      "target": "ES2020",
      "useDefineForClassFields": true,
      "lib": ["ES2020", "DOM", "DOM.Iterable"],
      "module": "ESNext",
      "moduleResolution": "Bundler",
      "resolveJsonModule": true,
      "jsx": "react-jsx",
  
      "noEmit": true,
      "skipLibCheck": true,
  
      "strict": false,
      "noImplicitAny": false,
      "checkJs": false
    },
    "include": ["src"]
  }
  